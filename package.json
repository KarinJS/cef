{
  "name": "@karinjs/screenshot-tools",
  "version": "0.1.0",
  "private": true,
  "description": "CEF and WebView2 based screenshot tools for Windows",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:cef": "cd packages/cef-node-addon && npm run build",
    "build:webview2": "cd packages/webview2-screenshot-tool && dotnet build -c Release",
    "demo:cef": "cd packages/cef-node-addon && node demo.js",
    "demo:webview2": "cd packages/webview2-screenshot-tool && node demo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarinJS/cef.git"
  },
  "keywords": [
    "cef",
    "webview2",
    "screenshot",
    "chromium",
    "windows"
  ],
  "author": "KarinJS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KarinJS/cef/issues"
  },
  "homepage": "https://github.com/KarinJS/cef#readme"
}